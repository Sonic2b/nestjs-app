{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rules": {
        "ordered-imports": [
            false
        ],
        "member-access": [
            false
        ],
        "member-ordering": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-implicit-dependencies": true,
        "no-unused-variable": true
    }
}